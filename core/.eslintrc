{
  "extends": "eslint-config-airbnb",
  "env": {
    "jquery": true,
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-alert": 0,
    "comma-dangle": [
      2,
      "never"
    ],
    "func-names": [
      "error",
      "never"
    ]
  },
  "globals": {
    "LoadMask": true,
    "noty": true
  }
}