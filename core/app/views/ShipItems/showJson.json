{
    "id": "${itm.id}",
    "qty": "${itm.unit.shipmentQty()}",
    "recivedQty": "${itm.recivedQty}",
    "shipDate": "${itm.shipDate.format('yyyy-MM-dd HH:mm:ss')}",
    "arriveDate": "${itm.arriveDate.format('yyyy-MM-dd HH:mm:ss')}",
    "fulfillmentNetworkSKU": "${itm.fulfillmentNetworkSKU}",
    "unit": {
        "sid": "${itm.unit?.sid}",
        "sku": "${itm.unit?.sku}",
